{
  "name": "qzd-utils",
  "version": "0.0.1",
  "description": "企知道通用工具类",
  "author": "citrus",
  "directories": {
    "test": "test"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "keywords": ["qizhidiao", "qzd"],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "tsc -w && node lib/index.js",
    "start": "nodemon --config nodemon.json lib/index.js",
    "doc": "typedoc --out docs/tsdoc/ --excludePrivate",
    "deprecated": "tsc -w && node lib/index.js"
  },
  "lint-staged": {
    "src/*.{ts,tsx}": ["eslint", "prettier --single-quote --write", "git add"],
    "{*.json,.{babelrc,eslintrc,prettierrc,stylelintrc}}": [
      "prettier --parser json --write",
      "git add"
    ],
    "*.{yml,md}": ["prettier --single-quote --write", "git add"]
  },
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
